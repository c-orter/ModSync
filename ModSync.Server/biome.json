{
	"$schema": "https://biomejs.dev/schemas/1.7.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"formatter": {
		"formatWithErrors": true
	},
	"linter": {
		"enabled": true,
		"ignore": ["src/crc.ts"],
		"rules": {
			"recommended": true,
			"complexity": {
				"useLiteralKeys": "off"
			}
		}
	}
}
